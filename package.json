{
  "name": "aurelia-acl",
  "version": "0.0.2",
  "description": "An ACL plugin to use with aurelia.",
  "scripts": {
    "test": "gulp test",
    "build": "gulp build",
    "preversion": "npm test",
    "version": "npm run build && gulp prepare-package && git add -A dist doc/CHANGELOG.md bower.json",
    "postpublish": "git push upstream master && git push upstream --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpoonX/aurelia-acl.git"
  },
  "keywords": [
    "aurelia",
    "acl",
    "permission",
    "permissions",
    "spoonx",
    "auth",
    "authentication"
  ],
  "homepage": "https://github.com/SpoonX/aurelia-acl#readme",
  "bugs": {
    "url": "https://github.com/SpoonX/aurelia-acl/issues"
  },
  "license": "MIT",
  "author": "SpoonX <info@spoonx.nl>",
  "contributors": [
    {
      "name": "Bassim Huis",
      "email": "bas080@hotmail.com"
    }
  ],
  "main": "dist/commonjs/aurelia-acl.js",
  "typings": "dist/aurelia-acl.d.ts",
  "jspm": {
    "jspmPackage": true,
    "registry": "npm",
    "main": "aurelia-acl",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "dependencies": {
      "aurelia-dependency-injection": "^1.2.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-pal": "^1.0.0",
      "aurelia-templating": "^1.0.0"
    },
    "devDependencies": {
      "aurelia-polyfills": "^1.0.0"
    },
    "peerDependencies": {
      "aurelia-dependency-injection": "^1.2.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-pal": "^1.0.0",
      "aurelia-templating": "^1.0.0"
    }
  },
  "devDependencies": {
    "spoonx-tools": "^1.0.0-0"
  },
  "aurelia": {
    "build": {
      "resources": []
    }
  },
  "dependencies": {
    "aurelia-dependency-injection": "^1.2.0",
    "aurelia-logging": "^1.0.0",
    "aurelia-pal": "^1.0.0",
    "aurelia-templating": "^1.0.0"
  }
}